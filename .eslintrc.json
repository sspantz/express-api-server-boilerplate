{
  "env": {
    "es2021": true,
    "node": true
  },
  "plugins": [
    "prettier",
    "functional",
    "jest"
  ],
  "extends": ["airbnb-base", "prettier", "plugin:functional/lite", "plugin:jest/recommended"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "jest/expect-expect": [
      1,
      {"assertFunctionNames": ["expect", "request"]}
    ]
  }
}
